    "test:unit": "vue-cli-service test:unit",
    "ssr:dev": "cross-env NODE_ENV=development PORT=9000 SECURE=9443 DEBUG=true node AppServer",
    "ssr:start": "cross-env NODE_ENV=production PORT=9000 SECURE=9443 DEBUG=false node AppServer",
    "ssr:build": "rimraf dist && npm run ssr:build:client && npm run ssr:build:server",
    "ssr:build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "ssr:build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules"
